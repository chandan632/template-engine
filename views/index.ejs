<%- include('partials/header') -%>
<%- include('partials/navbar') -%>
<img src="images/chandan.jpg" alt="chandan" width="200" height="150">
<h1>
    <%= data.name %>
</h1>
<h1>
    <%= data.age %>
</h1>
<h1>
    <%= data.address %>
</h1>
<ul>
    <% data.cities.forEach( city => {  %>
    <li><%= city %></li>
